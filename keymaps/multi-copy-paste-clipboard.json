{
  "atom-workspace": {
    "ctrl-alt-y": "multi-copy-paste:toggle",
    "ctrl-alt-i": "multi-copy-paste:paste",
    "ctrl-shift-x": "multi-copy-paste:copy",
    "ctrl-shift-f1": "multi-copy-paste:paste-1"

  },
  "atom-panel":{
    "escape": "multi-copy-paste:toggle"
  },
  "input#input.native-key-bindings":{
    "enter": "multi-copy-paste:toggle"

  }
}
